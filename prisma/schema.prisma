datasource db { provider = "postgresql" url = env("DATABASE_URL") }
generator client { provider = "prisma-client-js" }

model User {
  id        Int     @id @default(autoincrement())
  email     String  @unique
  password  String
  role      String
  createdAt DateTime @default(now())
}

model Service {
  id    Int    @id @default(autoincrement())
  slug  String @unique
  title String
  short String
  long  String
}